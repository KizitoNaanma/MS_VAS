{
  "name": "religious-notifications-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node src/shared/database/prisma/seeders/index.ts"
  },
  "lint-staged": {
    "*.{js,ts, md, json}": [
      "prettier --write .",
      "eslint --fix"
    ]
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:create": "npx prisma migrate dev --schema src/shared/database/prisma/schema.prisma --create-only",
    "migrate:dev": "npx prisma migrate dev --schema src/shared/database/prisma/schema.prisma",
    "migrate:deploy": "npx prisma migrate deploy --schema src/shared/database/prisma/schema.prisma",
    "schema:generate": "npx prisma generate --schema=src/shared/database/prisma/schema.prisma",
    "schema:generate:sql": "npx prisma generate --schema=src/shared/database/prisma/schema.prisma --sql",
    "schema:generate:types": "npx prisma generate --schema=src/shared/database/prisma/schema.prisma --no-engine",
    "schema:generate:sql-types": "npx prisma generate --schema=src/shared/database/prisma/schema.prisma --no-engine --sql",
    "prisma:studio": "npx prisma studio --schema=src/shared/database/prisma/schema.prisma",
    "prettier": "prettier --write .",
    "prepare": "husky && husky install",
    "prettier:silent": "prettier --write . --log-level silent",
    "seed:religions": "npx prisma db seed -- --type religions",
    "seed:themes": "npx prisma db seed -- --type themes",
    "seed:daily-prayers": "npx prisma db seed -- --type daily-prayers",
    "seed:daily-scriptures": "npx prisma db seed -- --type daily-scriptures",
    "seed:daily-devotionals": "npx prisma db seed -- --type daily-devotionals",
    "seed:course-categories": "npx prisma db seed -- --type course-categories",
    "seed:course-topics": "npx prisma db seed -- --type course-topics",
    "seed:courses": "npx prisma db seed -- --type courses",
    "seed:mindfulness-resources-categories": "npx prisma db seed -- --type mindfulness-resources-categories",
    "seed:mindfulness-resources": "npx prisma db seed -- --type mindfulness-resources",
    "seed:artists": "npx prisma db seed -- --type artists",
    "seed:albums": "npx prisma db seed -- --type albums",
    "seed:tracks": "npx prisma db seed -- --type tracks",
    "seed:quotes": "npx prisma db seed -- --type quotes",
    "seed:daily-quizzes": "npx prisma db seed -- --type daily-quizzes",
    "seed:marketers": "npx prisma db seed -- --type marketers",
    "seed:all": "yarn seed:religions && yarn seed:themes && yarn seed:daily-prayers && yarn seed:daily-scriptures && yarn seed:daily-devotionals && yarn seed:course-categories && yarn seed:course-topics && yarn seed:courses && yarn seed:mindfulness-resources-categories && yarn seed:mindfulness-resources && yarn seed:artists && yarn seed:albums && yarn seed:tracks && yarn seed:quotes && yarn seed:daily-quizzes",
    "trivia-engine-types:generate": "openapi-typescript https://api-trivia-dev.glorify.africa/openapi.json -o src/common/types/generated/openapi-schema.d.ts"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/nestjs": "^6.1.0",
    "@adminjs/prisma": "^5.0.3",
    "@aws-sdk/client-s3": "^3.677.0",
    "@aws-sdk/s3-request-presigner": "^3.732.0",
    "@brakebein/prisma-generator-nestjs-dto": "1.23.3",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.0.7",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/websockets": "^11.0.7",
    "@node-rs/bcrypt": "^1.10.4",
    "@prisma/client": "^5.22.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "6.4.16",
    "adminjs": "^7.8.13",
    "axios": "^1.7.7",
    "bullmq": "^5.41.7",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express-formidable": "^1.2.0",
    "express-session": "^1.18.1",
    "generate-password": "^1.7.1",
    "google-auth-library": "^9.13.0",
    "googleapis": "^140.0.1",
    "jsonwebtoken": "^9.0.2",
    "mailgen": "^2.0.28",
    "morgan": "^1.10.0",
    "nest-winston": "^1.9.7",
    "nodemailer": "^6.9.14",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "uuid": "^13.0.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@commitlint/types": "^19.5.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "15.2.2",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.0.0",
    "prisma": "^5.22.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
